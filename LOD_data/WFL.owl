    <!-- http://lila-erc.eu/ontologies/lila/wfl/Prefixation -->

    <owl:Class rdf:about="http://lila-erc.eu/ontologies/lila/wfl/Prefixation">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://lila-erc.eu/ontologies/lila/wfl/involves"/>
                <owl:someValuesFrom rdf:resource="http://lila-erc.eu/ontologies/lila/Prefix"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://lila-erc.eu/ontologies/lila/wfl/DerivationalRule"/>
        <rdfs:label>Prefixation</rdfs:label>
    </owl:Class>
    


