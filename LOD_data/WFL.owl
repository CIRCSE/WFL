    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://lila-erc.eu/ontologies/lila/wfl/hasPosInput -->

    <owl:ObjectProperty rdf:about="http://lila-erc.eu/ontologies/lila/wfl/hasPosInput">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://lila-erc.eu/ontologies/lila/wfl/WFLRule"/>
        <rdfs:range rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo#PartOfSpeech"/>
        <rdfs:comment>Rules have certain PoS as input, e.g.: v-to-v has verb as input</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://lila-erc.eu/ontologies/lila/wfl/hasPosOutput -->

    <owl:ObjectProperty rdf:about="http://lila-erc.eu/ontologies/lila/wfl/hasPosOutput">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://lila-erc.eu/ontologies/lila/wfl/WFLRule"/>
        <rdfs:range rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo#PartOfSpeech"/>
        <rdfs:comment>Rules have certain PoS as output, e.g.: v-to-v has verb as input</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://lila-erc.eu/ontologies/lila/wfl/hasWordFormationRule -->

    <owl:ObjectProperty rdf:about="http://lila-erc.eu/ontologies/lila/wfl/hasWordFormationRule">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:range rdf:resource="http://lila-erc.eu/ontologies/lila/wfl/WFLRule"/>
        <rdfs:comment>Property that relates morph:DerivationalRelation to wfl:WFLRule</rdfs:comment>
        <rdfs:comment>TODO: when morph module is published, set the domain explicitely</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://lila-erc.eu/ontologies/lila/wfl/involves -->

    <owl:ObjectProperty rdf:about="http://lila-erc.eu/ontologies/lila/wfl/involves">
        <rdfs:domain rdf:resource="http://lila-erc.eu/ontologies/lila/wfl/WFLRule"/>
        <rdfs:range rdf:resource="http://lila-erc.eu/ontologies/lila/Affix"/>
        <rdfs:comment>RIDEFINIRE il NOME!!!</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://lila-erc.eu/ontologies/lila/wfl/positionInWFR -->

    <owl:ObjectProperty rdf:about="http://lila-erc.eu/ontologies/lila/wfl/positionInWFR">
