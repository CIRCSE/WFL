    </owl:Class>
    


    <!-- http://lila-erc.eu/ontologies/lila/wfl/Backformation -->

    <owl:Class rdf:about="http://lila-erc.eu/ontologies/lila/wfl/Backformation">
        <owl:equivalentClass rdf:resource="http://lila-erc.eu/ontologies/lila/wfl/Conversion"/>
        <rdfs:subClassOf rdf:resource="http://lila-erc.eu/ontologies/lila/wfl/DerivationalRule"/>
        <rdfs:label>Backformation</rdfs:label>
    </owl:Class>
    


    <!-- http://lila-erc.eu/ontologies/lila/wfl/CompoundingRule -->

    <owl:Class rdf:about="http://lila-erc.eu/ontologies/lila/wfl/CompoundingRule">
        <rdfs:subClassOf rdf:resource="http://lila-erc.eu/ontologies/lila/wfl/WFLRule"/>
        <owl:disjointWith rdf:resource="http://lila-erc.eu/ontologies/lila/wfl/DerivationalRule"/>
        <rdfs:label>Compounding word-formation rule</rdfs:label>
    </owl:Class>
    


    <!-- http://lila-erc.eu/ontologies/lila/wfl/Conversion -->

    <owl:Class rdf:about="http://lila-erc.eu/ontologies/lila/wfl/Conversion">
        <rdfs:subClassOf rdf:resource="http://lila-erc.eu/ontologies/lila/wfl/DerivationalRule"/>
        <rdfs:comment>Conversion: derivation that does NOT involve Affixes.</rdfs:comment>
        <rdfs:label>Conversion</rdfs:label>
    </owl:Class>
    


